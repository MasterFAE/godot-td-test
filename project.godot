config_version=5

[application]

config/name="td-test"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

CurrencyManager="*res://script/global/currency_manager.gd"
UiSignalBus="*res://script/global/ui_signal_bus.gd"
GameSignalBus="*res://script/global/game_signal_bus.gd"
ExperienceManager="*res://script/global/experience_manager.gd"
PowerupManager="*res://script/global/powerup_manager.gd"

[display]

window/size/viewport_width=1000
window/size/viewport_height=600
window/size/initial_position_type=0
window/size/window_width_override=1280
window/size/window_height_override=760
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="td-test"

[editor_plugins]

enabled=PackedStringArray("res://addons/log/plugin.cfg", "res://addons/reload_current_scene/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "orange",
"res://scene/": "green",
"res://script/": "pink"
}

[global_group]

ground_enemy=""
player_unit_ground=""
